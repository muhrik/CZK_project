ctmc

const int k = 50;
const int qCapacity = 10;

const double timeout = 10;
const double lambda = 0.2;



module main

qSize : [0..qCapacity] init 0;

[produce] (qSize < qCapacity) -> (qSize' = qSize+1);
[consume] (qSize > 0) -> lambda: (qSize' = qSize - 1);

endmodule

module trigger

i : [1..k+1];

[] i < k -> k/timeout : (i'=i+1);
[produce] i = k -> k/timeout : (i'=1);
//[consume] true -> (i'=1);

endmodule
